        "illuminate/cache": "^12.21",
        "illuminate/collections": "^12.21",
        "illuminate/config": "^12.21",
        "illuminate/console": "^12.21",
        "illuminate/container": "^12.21",
        "illuminate/contracts": "^12.21",
        "illuminate/database": "^12.21",
        "illuminate/encryption": "^12.21",
        "illuminate/events": "^12.21",
        "illuminate/filesystem": "^12.21",
        "illuminate/hashing": "^12.21",
        "illuminate/http": "^12.21",
        "illuminate/macroable": "^12.21",
